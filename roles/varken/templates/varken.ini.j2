[global]
sonarr_server_ids = 1
radarr_server_ids = 1
ombi_server_ids = 1
tautulli_server_ids = 1
lidarr_server_ids = false
sickchill_server_ids = false
unifi_server_ids = false

maxmind_license_key = {{ maxmind_license_key }}

[influxdb]
url = {{ influxdb_url }}
port = {{ influxdb_port }}
ssl = true
verify_ssl = false
username = {{ varken_influxdb_username }}
password = {{ varken_influxdb_password }}

[tautulli-1]
url = {{ tautulli_url }}:{{ tautulli_port }}
apikey = {{ tautulli_api_key }}
ssl = true
verify_ssl = false
get_activity = true
get_activity_run_seconds = 30
get_stats = true
get_stats_run_seconds = 3600

[sonarr-1]
url = {{ sonarr_url }}:{{ sonarr_port }}
apikey = {{ sonarr_api_key }}
ssl = true
verify_ssl = false
missing_days = 7
missing_days_run_seconds = 300
future_days = 1
future_days_run_seconds = 300
queue = true
queue_run_seconds = 300

[radarr-1]
url = {{ radarr_url }}:{{ radarr_port }}
apikey = {{ radarr_api_key }}
ssl = true
verify_ssl = false
queue = true
queue_run_seconds = 300
get_missing = true
get_missing_run_seconds = 300

[ombi-1]
url = {{ ombi_url }}
apikey = {{ ombi_api_key }}
ssl = true
verify_ssl = false
get_request_type_counts = true
request_type_run_seconds = 300
get_request_total_counts = true
request_total_run_seconds = 300
get_issue_status_counts = true
issue_status_run_seconds = 300